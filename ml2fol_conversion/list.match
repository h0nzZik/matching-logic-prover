(declare-sort Nat)

(declare-func zero () Nat)
(declare-func succ (Nat) Nat)
(declare-func plus (Nat Nat) Nat)

(assert (forall ((X Nat))
  (= X (plus X zero))))

(assert (forall ((X Nat) (Y Nat))
  (= (succ (plus X Y))
     (plus X (succ Y)))))

(assert (not
  (= (succ (succ zero))
     (plus (succ zero) (succ zero)))))

